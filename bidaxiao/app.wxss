/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
::-webkit-scrollbar{
  width:0;
  height: 0;
}
image{
  width: 100%;
}
.bgcolor{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0; 
}
.indexmain{
  position: relative;
  top: 0;
  left: 0;
}
.tili{
  position: absolute; 
  top: 35rpx;
  left: 16rpx;
  width: 344rpx;
  height: 78rpx;
}
.tili image{
  width: 344rpx;
  height: 78rpx;
}
.tili text{
  display: block;
  position: absolute; 
  top: 23rpx;
  left: 98rpx;
  background: #ed4141;
  width: 230rpx;
  height: 39rpx;
  border-radius: 20rpx;
}
.activ{
  position: absolute;
  top: 34rpx;
  right: 0;
  width: 162rpx;
  height: 63rpx;
}
.activ image{
  width: 162rpx;
  height: 63rpx;
}
.title{
  position: relative;
  padding-top: 147rpx;
  text-align: center;
}
.title image{
  width: 556rpx;
  height: 228rpx;
}
.caiquan{
  position: relative;
  top: 38rpx;
  left: 0;
  right: 0;
  width: 750rpx;
  height: 338rpx;
}
.leftmain{
  position: absolute;
  top: 0;
  left: 71rpx;
}
.leftmain image,.rightmain image{
  width: 192rpx;
  height: 197rpx;
}
.rightmain{
  position: absolute;
  top: 0;
  right: 60rpx;
}
.userinfo{
  position: relative;
  top: 48rpx;
  left: 0;
}
.userinfo::after{
  content: '';
  position: absolute;
  top: -50rpx;
  left: 40%;
  transform: translateX(-50%);
  right: 0;
  width: 18rpx;
  height: 18rpx;
  background: #f2b40a;
  border-radius: 50%;
}
.userinfo::before{
  content: '';
  position: absolute;
  top: -20rpx;
  left: 40%;
  transform: translateX(-50%);
  right: 0;
  width: 11rpx;
  height: 11rpx;
  background: #f2b40a;
  border-radius: 50%;
}
.userinfo image{
  position: absolute;
  top: 0;
  left: 30rpx;
  float: left;
  width: 98rpx;
  height: 98rpx;
  border-radius: 50rpx;
}
.userinfo text{
  position: absolute;
  top: 30rpx;
  left: 135rpx;
  font-size: 27rpx;
  color: #fff;
  width: 100rpx;
}
.rightmain text{
  position: absolute;
  top: 30rpx;
  left: -55rpx;
  font-size: 27rpx;
  color: #fff;
  width: 100rpx;
}
.footer{
  position: relative;
  top: 96rpx;
  left: 0;
}
.guize{
  text-align: center;
  font-size: 28rpx;
  color: #fff;
}
.label{
  position: absolute;
  top: 3rpx;
  display: inline-block;
  width: 30rpx;
  height: 30rpx;
  margin-right: 15rpx;
  background:url(data:image/png;base64,) no-repeat;
  background-size:100% 100%;
}
.label.active{
  background:url(data:image/png;base64,) no-repeat;
  background-size:100% 100%;
}
.guize text{
  margin-left: 42rpx;
}
.cqbtn{
  margin: 25rpx auto;
  text-align: center;
}
.cqbtn image{
  width: 588rpx;
  height: 98rpx;
}
.time{
  margin: 0 auto;
  text-align: center;
  font-size: 26rpx;
  color: rgba(213,228,254,0.96);
}
.record{
   margin-top: 40rpx; 
  text-align: center
}
.record image{
  width: 202rpx;
  height: 90rpx;
}
.recordbg{
  background: #3aa1f5;
  padding: 25rpx 0;
}
.libg{
  position: relative;
  margin: 25rpx auto;
  width: 710rpx;
  height: 284rpx;
}
.r_time{
  font-size: 28rpx;
  color: #fff;
  padding-left: 38rpx;
  padding-top: 38rpx;
}
.r_icon{
  width: 110rpx;
  height: 113rpx;
}
.r_vs{
  position: relative;
  bottom: 15px;
  width: 73rpx;
  height: 49rpx;
  margin: 0 30rpx;
}
.r_main{
  position: absolute;
  bottom: 28rpx;
  left: 0;
  right: 0;
  margin: 0 auto;
  padding: 0 23rpx;
}
.r_main label{
  display: inline-block;
  position: relative;
  bottom: 20px;
  font-size: 28rpx;
  color: #fff;
  margin: 0 8rpx;
  max-width: 80rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.resultbtn{
  position: absolute;
  right: 28rpx;
  bottom: 8rpx;
}
.resultbtn image{
  width: 152rpx;
  height: 100rpx;
}
.hink{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.5);
}
.h_main{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  -webkit-transform: translate(-50%,-50%);
  width: 486rpx;
  height: 603rpx;
  margin: 0 auto;
}
.h_close{
  position: absolute;
  top: 38rpx;
  right: 0rpx;
  width: 61rpx;
  height: 63rpx;
}
.tips{
  margin-top: 150rpx;
  text-align: center;
}
.tips image{
  width: 168rpx;
  height: 86rpx;
}
.h_main text{
  display: block;
  margin: 0 auto;
  padding-top:60rpx; 
  text-align: center;
  font-size: 30rpx;
  color: #e49c2e;
}
.h_btn{
  margin: 50rpx auto auto auto;
  text-align: center;
  width: 280rpx;
  height: 100rpx;
  line-height: 100rpx;
  color: #fff;
  font-size: 36rpx;
  background: #ffe248;
  box-shadow: 0px 5px 0px 0px rgba(244, 146, 21, 0.47);
  border-radius: 50rpx;
  -webkit-border-radius:50rpx;
}
.show{
  display: block;
}