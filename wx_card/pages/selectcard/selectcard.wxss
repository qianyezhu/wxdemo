/* pages/info.wxss */

image{
  width: 100%;
  height: 100%
}
.greybody{
  background: #f5f5f5;
  padding-bottom: 30rpx;
}
.head{
  width: 750rpx;
  height: 220rpx;
}
.pb30{
  padding-bottom: 30rpx;
}

.spanleft{
  padding-left: 30rpx;
  font-size: 28rpx;
  color: #232323;
}
.spanleft text{
  color: #fe5733;
}
.spanright{
  position: absolute;
  top: 0;
  right: 56rpx;
  font-size: 28rpx;
  color: #232323;
}
.infomain{
  background: #fff;
}
.infitel{
  position: relative;
  margin-top: 20rpx;
  width: 750rpx;
  line-height: 80rpx;
  border-bottom: 1rpx solid #e5e5e5;
}
.spanleft{
  line-height: 80rpx;
  font-size: 28rpx;
  color: #232323;
}
.spanleft image{
  width: 6rpx;
  height: 24rpx;
  padding-right: 8rpx;
}
.spannew{
  position: absolute;
  top: 0;
  right: 29rpx;
  line-height: 80rpx;
}
.spannew image{
  padding-top: 19rpx;   
  width: 46rpx;
  height: 46rpx;
  display: inline-block;
  margin-right: 8rpx;
}
.spannew text{
  float: right;
  color: #232323;
  font-size: 28rpx;
}
.tellist{
  overflow: hidden
}
.tellist view{
  position: relative;
  float: left;
  margin-left: 30rpx;
  margin-top: 29rpx;
  border: 2rpx solid #999;
  width: 330rpx;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  border-radius: 5rpx;
  -webkit-border-radius: 5rpx;
  color: #232323;
  font-size: 28rpx;
}
.tellist .active{
  position: relative;
  color: #fe5733;
  border: 2rpx solid #fe5733;
}
.tellist .active::after{
  display: block;
  content: '';
  width: 52rpx;
  height: 52rpx;
  position: absolute;
  right: 0;
  bottom: 0;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
}
.grey::before{
  content: '已被预约';
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.4);
  color:#fff;
}
.telbtn{
  width: 690rpx;
  height: 80rpx;
  background: #fe5733;
  text-align: center;
  color: #fff;
  line-height: 80rpx;
  font-size: 30rpx;
  margin: 30rpx auto auto auto;
  border-radius: 13rpx;
  -webkit-border-radius:13rpx;
}
.footer{
  margin-top: 30rpx;
  background: #fff;
}
.ptext{
  color: #232323;
  font-size: 24rpx;
  margin: 0 30rpx;
  line-height: 50rpx;
}